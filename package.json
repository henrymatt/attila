{
  "name": "fancy",
  "description": "Theme for Ghost, forked from Attila",
  "version": "1.0.2",
  "engines": {
    "ghost": ">=2.1.0",
    "ghost-api": "v2"
  },
  "license": "MIT",
  "author": {
    "name": "Matt Henry",
    "email": "matt@henrymatt.com",
    "url": "https://www.henrymatt.com"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Minimal",
      "Personal Blogs"
    ]
  },
  "keywords": [
    "ghost-theme",
    "ghost",
    "theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/henrymatt/fancy.git"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "grunt": "^1.0.4",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^2.1.0",
    "load-grunt-tasks": "^3.5.2"
  },
  "config": {
    "posts_per_page": 10,
    "image_sizes": {
      "s": {
        "width": 320
      },
      "m": {
        "width": 640
      },
      "l": {
        "width": 960
      },
      "xl": {
        "width": 1920
      }
    }
  },
  "browserslist": [
    "defaults"
  ]
}
